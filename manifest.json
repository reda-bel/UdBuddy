{
 "manifest_version": 2,
 "name": "Udemy support plugin",
 "description": "This extension should help handle tickets more easily.",
 "version": "1.0",
 
 "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "https://ajax.googleapis.com/",
   "https://udemysupport.zendesk.com/*",
   "https://www.udemy.com/*"
  ]


}